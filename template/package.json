{
   "name": "ReactNativeProject",
   "version": "0.0.1",
   "private": true,
   "scripts": {
      "android": "react-native run-android",
      "ios": "react-native run-ios",
      "start": "react-native start",
      "test": "jest",
      "lint": "eslint .",
      "podinstall": "cd ios && pod install && cd ..",
      "format": "prettier --write \"./src/**/*.{js,md,yml}\"",
      "build:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && react-native run-android",
      "bundle:ios": "react-native bundle --dev false --entry-file index.js --bundle-output ios/main.jsbundle --platform ios --assets-dest ios",
      "clean": "watchman watch-del-all && rm -rf package-lock.json yarn.lock && rm -rf node_modules && rm -rf $TMPDIR/metro-* && rm -rf $TMPDIR/haste-map-* && yarn install && say Project has been cleaned successfully"
   },
   "dependencies": {
      "@react-native-async-storage/async-storage": "^1.15.9",
      "@react-native-community/geolocation": "^2.0.2",
      "@react-native-community/masked-view": "^0.1.10",
      "@react-native-community/netinfo": "^5.9.10",
      "@react-navigation/bottom-tabs": "^6.0.9",
      "@react-navigation/native": "^5.8.10",
      "@react-navigation/stack": "^5.14.2",
      "@reduxjs/toolkit": "^1.5.0",
      "axios": "^0.21.1",
      "formik": "^2.2.9",
      "lodash": "^4.17.21",
      "native-base": "^3.2.1",
      "prop-types": "^15.7.2",
      "react": "17.0.1",
      "react-native": "0.64.1",
      "react-native-bootsplash": "^3.2.6",
      "react-native-fs": "^2.18.0",
      "react-native-gesture-handler": "^1.9.0",
      "react-native-htmlview": "^0.16.0",
      "react-native-maps": "^0.29.3",
      "react-native-modalize": "^2.0.10",
      "react-native-offline": "^6.0.0",
      "react-native-photo-upload": "^1.3.0",
      "react-native-portalize": "^1.0.7",
      "react-native-safe-area-context": "^3.1.9",
      "react-native-screens": "^2.16.1",
      "react-native-size-matters": "^0.4.0",
      "react-native-snap-carousel": "^3.9.1",
      "react-native-storage": "^1.0.1",
      "react-native-vector-icons": "^7.1.0",
      "react-query": "^3.5.16",
      "react-redux": "^7.2.2",
      "redux": "^4.0.5",
      "redux-persist": "^6.0.0",
      "redux-thunk": "^2.3.0",
      "styled-components": "^5.3.3",
      "styled-system": "^5.1.5",
      "yup": "^0.32.11"
   },
   "devDependencies": {
      "@babel/core": "^7.16.0",
      "@babel/runtime": "^7.16.0",
      "@react-native-community/eslint-config": "^3.0.1",
      "babel-jest": "^27.3.1",
      "eslint": "^8.1.0",
      "jest": "^27.3.1",
      "metro-react-native-babel-preset": "^0.66.2",
      "react-test-renderer": "17.0.2"
   },
   "jest": {
      "preset": "react-native"
   },
   "workspaces": [
      "./pkgs/*"
   ]
}
